<?xml version="1.0" encoding="UTF-8"?>
<glossary class="glossary">
 <title class="title">Abbreviations</title>
 <def-list class="def-list">
  <def-item class="def-item">
   <term class="term">ATCC</term>
   <def class="def">
    <p class="p">American type culture collection</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">DMSO</term>
   <def class="def">
    <p class="p">Dimethyl sulfoxide</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">DPPH</term>
   <def class="def">
    <p class="p">2,2-diphenyl-1-picrylhydrazyl</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">GC-MS</term>
   <def class="def">
    <p class="p">Gas chromatography mass spectrometry</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">GRAS</term>
   <def class="def">
    <p class="p">Generally recognized as safe</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">IC
    <sub class="sub">50</sub>
   </term>
   <def class="def">
    <p class="p">Half-maximal inhibitory concentration</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">MIC</term>
   <def class="def">
    <p class="p">Minimal inhibitory concentration</p>
   </def>
  </def-item>
 </def-list>
</glossary>
